#!/bin/sh

#
# Program: vocload.config
#
# Purpose:
#
#   Configuration file for Protein Isoform Ontology
#
# History:
#

cd `dirname $0` 

RUNTIME_DIR=${OUTPUTDIR}
ARCHIVE_DIR=${ARCHIVEDIR}

export RUNTIME_DIR
export ARCHIVE_DIR

LOAD_PROGRAM="${LOAD_PROGRAM}"
export LOAD_PROGRAM

case `uname -n` in
bhmgiapp01) MAINTAINER="mgiadmin";;
*) MAINTAINER="$USER";;
esac
export MAINTAINER

# vocabulary attributes:

VOCAB_NAME="Protein Isoform Ontology"
VOCAB_COMMENT_KEY=0
JNUM=${JNUMBER}
IS_SIMPLE=1
IS_PRIVATE=0
LOGICALDB_KEY=183
ANNOT_TYPE_KEY=0

export VOCAB_NAME
export VOCAB_COMMENT_KEY
export JNUM
export IS_SIMPLE
export IS_PRIVATE
export LOGICALDB_KEY
export ANNOT_TYPE_KEY

DATA_FILE=${INPUTDIR}/provoc.txt
TERM_FILE="${RUNTIME_DIR}/provocload.txt"
DAG_ROOT_ID=""
export DATA_FILE
export TERM_FILE
export DAG_ROOT_ID

